{
  "name": "osm-mapmaking",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@juno/toolbox": "git+ssh://git.junolab.net/fl/toolbox.git",
    "@juno/types": "git+ssh://git.junolab.net/fl/types.git",
    "@juno/ui-private": "git+ssh://git.junolab.net/fl/ui-private.git",
    "mapbox-gl": "^1.5.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-mapbox-gl": "^4.7.3"
  },
  "devDependencies": {
    "@avg206/frontend-config": "github:avg206/frontend-config",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0"
  },
  "scripts": {
    "build": "juno-scripts build",
    "start": "node mocks"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": "./node_modules/@avg206/frontend-config/eslint/javascript.js"
  },
  "eslintIgnore": [
    "node_modules",
    "dist"
  ],
  "prettier": "@avg206/frontend-config/prettier",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
